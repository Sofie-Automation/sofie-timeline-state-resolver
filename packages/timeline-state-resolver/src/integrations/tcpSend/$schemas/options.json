{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "TCP Send Options",
	"type": "object",
	"properties": {
		"host": {
			"type": "string",
			"ui:title": "Host"
		},
		"port": {
			"type": "integer",
			"ui:title": "Port"
		},
		"bufferEncoding": {
			"type": "string",
			"ui:title": "Buffer Encoding",
			"enum": ["ascii", "utf8", "utf-8", "utf16le", "ucs2", "ucs-2", "base64", "base64url", "latin1", "binary", "hex"],
			"default": "utf8"
		}
	},
	"required": ["host", "port"],
	"additionalProperties": false
}
